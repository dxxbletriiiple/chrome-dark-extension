{
	"manifest_version": 3,
	"name": "Dark",
	"description": "Dark theme for chrome settings on Ubuntu",
	"version": "1.0",
	"icons": {
		"16": "images/16.png",
		"32": "images/32.png",
		"48": "images/48.png",
		"64": "images/64.png",
		"128": "images/128.png"
	},
	"content_scripts": [
		{
			"matches": ["<all_urls>", "<chrome://>"],
			"css": ["style.css"]
		}
	]
}
